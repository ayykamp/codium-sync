        "textMateRules": [
          {
            "scope": [
              "comment",
              "entity.name.type.class",
              "keyword",
              "constant",
              "storage.modifier",
              "storage.type.class.js"
            ],
            "settings": {
              "fontStyle": "italic"
            }
          },
          {
            "scope": [
              "invalid",
              "keyword.operator",
              "constant.numeric.css",
              "keyword.other.unit.px.css",
              "constant.numeric.decimal.js",
              "constant.numeric.json"
            ],
            "settings": {
              "fontStyle": ""
            }
          }
        ]
      },
      "workbench.colorTheme": "Atom One Dark",
